.personContainer {
  height: 100vh;
  width: 100vh;
  background: #eee;
}
.personContainer .header {
  position: relative;
  height: 268rpx;
  width: 100%;
  background: #f4ca7f;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.personContainer .header .user {
  position: absolute;
  left: 30rpx;
  top: 63rpx;
  width: 142rpx;
  height: 142rpx;
  border-radius: 50%;
  background: #fff;
}
.personContainer .header .text {
  position: absolute;
  left: 200rpx;
  top: 95rpx;
}
.personContainer .data {
  height: 285rpx;
  width: 100%;
  background: #fff;
}
.personContainer .data .money {
  width: 100%;
  height: 105rpx;
  border-bottom: 1rpx solid #eee;
  line-height: 105rpx;
  margin-left: 30rpx;
}
.personContainer .data .list {
  height: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
}
.personContainer .data .list .listItem {
  font-size: 24rpx;
  height: 180rpx;
  width: 13.5%;
  text-align: center;
  margin-top: 50rpx;
}
.personContainer .dataNext {
  height: 20rpx;
  background: #eee;
}
.personContainer .menu {
  height: 723rpx;
  width: 750rpx;
  background: #fff;
}
.personContainer .menu .menuContainer {
  height: 180rpx;
  width: 248rpx;
  float: left;
  border-right: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  text-align: center;
}
.personContainer .menu .menuContainer .menuItem {
  margin-top: 30rpx;
}
.personContainer .menu .menuContainer .menuItem image {
  width: 50rpx;
  height: 50rpx;
}
.personContainer .menuNext {
  height: 40rpx;
  background: #eee;
}
.personContainer .quit {
  height: 96rpx;
  width: 100%;
  background: #fff;
  border: 1rpx solid #eee;
}
.personContainer .quit .btn {
  line-height: 96rpx;
  margin-left: 320rpx;
}
.personContainer .quitNext {
  height: 20rpx;
  background: #eee;
}

